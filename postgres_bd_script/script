/* Execute first */
CREATE DATABASE student_tracker; 

/* Execute inside student_tracker database */
CREATE TABLE 'student' (
	id SERIAL PRIMARY KEY,
	first_name VARCHAR(45),
	last_name VARCHAR(45),
	email VARCHAR(45)
);
